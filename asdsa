document.addEventListener("DOMContentLoaded", () => {

let imagenes = []

imagenes[0] = "img/zed-relamp.jpg";
imagenes[1] = "img/proyecto.jpg";
imagenes[2] = "img/decamp.jpg"
imagenes[3] = "img/zed-jur.jpg";
imagenes[4] = "img/zed-slayer.jpg";
imagenes[5] = "img/zed-psy.jpg";
imagenes[6] = "img/zed-prestigio.jpg"


let indiceDeImagenes = 0;

function cambiarImg () {

    document.slider.src = imagenes[indiceDeImagenes];

    if(indiceDeImagenes < 6){
        indiceDeImagenes++
    }else{
        indiceDeImagenes = 0;
    }
}

setInterval(cambiarImg, 2000)


let iconos = [];
iconos[0] = "img/shuriken-2.png"
iconos[1] = "img/proyect.png"
iconos[2] = "img/shuriken.png";
iconos[3] = "img/garra.png";
iconos[4] = "img/slayer.jpg";
iconos[5] = "img/psy.jpg";
iconos[6] = "img/prestigio.jpg"



let indiceIconos = 0;

function cambiarIcon () {
    document.iconos.src = iconos[indiceIconos]

    if(indiceIconos < 6){
        indiceIconos++
    }else{
        indiceIconos = 0;
    }

    

}

setInterval(cambiarIcon, 2000)

})

document.querySelector(".menu-btn").addEventListener("click", () => {
    document.querySelector(".menu-ul").classList.toggle("tr");
})



ScrollReveal().reveal(".bio", { delay: 500 });
ScrollReveal().reveal(".cards-1", { delay: 500 });
ScrollReveal().reveal(".container-skins", { delay: 500 });
